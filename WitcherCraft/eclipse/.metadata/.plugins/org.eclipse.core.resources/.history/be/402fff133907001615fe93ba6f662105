	private ItemStack pickCheckLootItem(Random random)
	{
		 int i = random.nextInt(2);
		 if (i == 0)
		 {
			 return new ItemStack(WitcherCraft.schematWolfSilverSword,1);
		 }
		 if (i == 1)
		 {
			 
			 return new ItemStack(WitcherCraft.crossbowWood,1);
		 }
		 if (i == 3)
		 {
			 
			 return new ItemStack(Items.arrow, random.nextInt(4) + 1);
		 }
		 else
		 {
			 return null;
		 }
	}
}