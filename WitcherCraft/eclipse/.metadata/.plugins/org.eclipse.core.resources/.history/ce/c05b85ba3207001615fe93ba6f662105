	public SchematWolfSilverSword() {
        maxStackSize = 64;
        setCreativeTab(WitcherCraft.tabWitcherSchematy);
        setTextureName(WitcherCraft.MODID + ":" + "schemat");
        setUnlocalizedName("schematWolfSilverSword");
